.row.form.login
  .col-md-8.col-md-offset-2.login-container
    %button.exit.pull-right.btn
      x
    .row
      %h2 Sign In
      = form_for(:session, url: logging_in_path) do |f|
        = f.text_field :email, class: "form-control", placeholder: "email"
        = f.password_field :password, class: "form-control", placeholder: "password"
        = f.submit "Login", class: "btn-lg pull-right"
